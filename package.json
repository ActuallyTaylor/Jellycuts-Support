{
	"name": "jellycuts-support",
	"displayName": "Jellycuts Support",
	"description": "Support for the Jellycuts iOS app.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:jellycuts-support.runserver",
		"onCommand:jellycuts-support.closeserver",
		"onLanguage:Jelly"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
			  "command": "jellycuts-support.runserver",
			  "title": "Open Jellycuts Bridge"
			},
			{
				"command": "jellycuts-support.closeserver",
				"title": "Close Jellycuts Bridge"
			}  
		],	  
		"keybindings": [
			{
				"command": "jellycuts-support.runOnDevice",
				"key": "ctrl+r",
				"mac": "cmd+r"
			},
			{
				"command": "jellycuts-support.exportOnDevice",
				"key": "ctrl+shift+e",
				"mac": "cmd+shift+e"
			}
		],
		"languages": [
			{
				"id": "Jelly",
				"aliases": [
					"Jelly",
					"jelly",
					"Jellycuts",
					"jellycut"
				],
				"extensions": [
					".jelly",
					".jellycut"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "Jelly",
				"scopeName": "source.jelly",
				"path": "./syntaxes/jelly.tmLanguage.json"
			}
		],
		"snippets": [
			{
			  "language": "Jelly",
			  "path": "./snippets/snippets.json"
			}
		  ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"internal-ip": "^6.2.0",
		"ws": "^7.4.2"
	}
}
